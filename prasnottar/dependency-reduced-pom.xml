<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>prasnottar</groupId>
  <artifactId>prasnottar</artifactId>
  <name>prasnottar</name>
  <version>1.0.0</version>
  <url>http://maven.apache.org</url>
  <build>
    <finalName>prasnottar</finalName>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>2.3.2</version>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>2.4.3</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration>
              <minimizeJar>true</minimizeJar>
              <filters>
                <filter>
                  <artifact>*:*</artifact>
                  <excludes>
                    <exclude>META-INF/*.SF</exclude>
                    <exclude>META-INF/*.DSA</exclude>
                    <exclude>META-INF/*.RSA</exclude>
                  </excludes>
                </filter>
              </filters>
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>org.projectlombok</groupId>
      <artifactId>lombok</artifactId>
      <version>1.18.0</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>edu.stanford.nlp</groupId>
      <artifactId>stanford-corenlp</artifactId>
      <version>3.9.1</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/contrib/stanford-corenlp/stanford-corenlp-3.9.1.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>edu.stanford.nlp</groupId>
      <artifactId>stanford-corenlp-model</artifactId>
      <version>3.9.1</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/contrib/stanford-corenlp/stanford-corenlp-3.9.1-models.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>de.jollyday</groupId>
      <artifactId>jollyday</artifactId>
      <version>0.4.9</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/contrib/stanford-corenlp/jollyday-0.4.9-sources.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>de.jollyday</groupId>
      <artifactId>jollyday</artifactId>
      <version>0.4.9</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/contrib/stanford-corenlp/jollyday.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>org.ejml</groupId>
      <artifactId>ejml</artifactId>
      <version>0.23</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/contrib/stanford-corenlp/ejml-0.23.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>javax.json</groupId>
      <artifactId>javax.json-api</artifactId>
      <version>1.0</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/contrib/stanford-corenlp/javax.json-api-1.0-sources.jar</systemPath>
    </dependency>
  </dependencies>
  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
</project>

